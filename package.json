{
  "name": "sass-scanner",
  "version": "0.0.0",
  "private": true,
  "description": "Want less Sass? Track it.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshamos/sass-tracker.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "JamOS",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.0",
    "sass-graph": "^3.0.5",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^12.7.12",
    "@types/sass-graph": "^2.1.2",
    "@types/semver": "^7.3.1",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.1.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "memfs": "^3.2.0",
    "prettier": "^2.0.5",
    "readdirp": "^3.4.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "unionfs": "^4.4.0"
  }
}
